# OpenAI API Key (optional - only needed if using OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Model options
# For OpenAI: gpt-3.5-turbo, gpt-4, etc.
# For local models: leave blank and modify code to use local LLM
MODEL_NAME=gpt-3.5-turbo

# Embedding model (HuggingFace model name)
EMBEDDING_MODEL_NAME=sentence-transformers/all-MiniLM-L6-v2

# Chunk size for text splitting
CHUNK_SIZE=1000

# Chunk overlap for text splitting
CHUNK_OVERLAP=200